# Sample environment variables for CrownWynn backend
# Copy this to .env and fill in secure values for local development.

# Django
DJANGO_SECRET_KEY=replace-with-a-secure-random-string
DJANGO_DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database (Postgres example)
# Format: postgres://user:password@host:port/dbname
DATABASE_URL=postgres://crownuser:crownpass@localhost:5432/crownwynn

# Security cookies
AUTH_COOKIE_SECURE=False
CSRF_COOKIE_SECURE=False
SESSION_COOKIE_SECURE=False
